pipeline {
    agent { label 'MAVEN'}

    stages {
        stage( 'git' ){
            steps{
                git url :' https://github.com/dhille98/spring-petclinic.git ',
                  branch: 'dev'
            }
        }
        stage('build'){
            steps{
                sh 'mvn clean package'
                archiveArtifacts artifacts: '**/spring-petclinic-*.jar'
                junit testResults: '**/TEST-*.xml'
            }
        }

    }
}